    function isComingFromGTM(e) {

        if (e.tagName !== 'SCRIPT')  {
            return false;
        }

        var stack = (new Error().stack).split('\n');

        if (stack.length === 0) {
            return false;
        }

        for(var i = 0; i < stack.length; i++) {
            if (stack[i].indexOf('https://www.googletagmanager.com/gtm.js') !== -1) {
                return true;
            }
        }

        return false;
    }
